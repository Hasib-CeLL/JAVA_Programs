package com.mycompany.linearsearch;

/**
 *
 * @author Student
 */
public class LinearSearch {
    
    
    public static int LS(int[] arr,int key){
    
    for(int i = 0 ; i<=arr.length ; i++)
            {
             if(arr[i] == key )
              {
               return i ;
              }
            
            }
    return -1 ;
    
    }

    public static void main(String[] args) {
        int[] arr1 = {1,2,3,4,5} ;
        int key = 5 ;
        System.out.println(key+" is found at index: "+LS(arr1, key)); 
        
      
    }
}
